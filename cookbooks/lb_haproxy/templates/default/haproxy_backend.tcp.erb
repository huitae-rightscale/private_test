backend <%= @backend_name_line %>

        mode tcp
        option tcplog

        balance <%= @algorithm %>
        
        # Timeout
        timeout server <%= @timeout_server %>

        # Haproxy status page
        <%= @stats_uri_line %>
        <%= @stats_auth_line %>

        # Special server line that allow HAProxy to start with cookies enabled and no valid servers.
        server disabled-server 127.0.0.1:1 disabled

